{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "rewrites": [
        { "source": "/favicon.ico", "destination": "/public/favicon.ico" }
      ]
    

    
  }
  